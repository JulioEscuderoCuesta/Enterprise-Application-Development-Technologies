

public abstract class Series {

    private String name;
    private LocalDate releaseDate;
    private String synopsis;
    private Set<String> creators;
    private Set<String> actors;
    private Categorie categorie;
    private Set<Season> seasons;

    public Series(String name, LocalDate releaseLocalDate, String synopsis, ArrayList<String> creators, ArrayList<String> actors, Categorie categorie, ArrayList<Season> seasons) {
        this.name = name;
        this.releaseLocalDate = releaseLocalDate;
        this.synopsis = synopsis;
        this.creators = creators;
        this.actors = actors;
        this.categorie = categorie;
        this.categorie = categorie;
        this.seasons = seasons;
    }

    //Getters & Setters
    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getReleaseLocalDate() {
        return releaseLocalDate;
    }

    public void setReleaseLocalDate(String releaseLocalDate) {
        return this.releaseLocalDate = releaseLocalDate;
    }

    public String getSynopsis() {
        return synopsis;
    }

    public void setSynopsis(String synopsis) {
        this.synopsis = synopsis;
    }

    public Set<String> getCreator() {
        return creators;
    }

    public Set<String> getActors() {
        return this.actors;
    }

    public Categorie getCategorie() {
        return categorie;
    }

    public void setCategorie() {
        this.categorie = categorie;
    }

    /**************************************************************** */

    public void watch() {
        // TODO:
    }
}